Config {
		font = "xft:SF Mono-10:regular,Noto Sans Mono CJK SC-10:regular"
		, alpha = 200
		, border = NoBorder
		, hideOnStart = False
		, position = Top
		, bgColor = "#232300"
		, fgColor = "#657b83"
		, template = " %StdinReader%}{%wlp2s0%|%battery%|%date% "
		, allDesktops = True

		, commands =
		[
			Run Battery [ "-t", "<left>%"] 150
			, Run StdinReader
			, Run Network "wlp2s0" ["-t", 
									"<fc=#00ff00>上:<tx></fc> <fc=#ff0000>下:<rx></fc>"
									, "-S", "True"] 50
			, Run Date "<fc=#ffa300>%a %b %d %T %Z</fc>" "date" 10
			]
}
