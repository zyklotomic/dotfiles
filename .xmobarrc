Config {
		font = "xft:SF Mono-10:bold,Noto Sans Mono CJK SC-10:bold"
                , additionalFonts = [ "xft:Font Awesome 5 Free:style=Solid:pixelsize=12" ]
		-- , alpha = 230
		, border = NoBorder
		, hideOnStart = False
		, position = Top
		, bgColor = "#361563"
		, fgColor = "#aac7d1"
		, template = " %StdinReader%}{%memory% %multicpu% %enp3s0% %date% "
		, allDesktops = True

		, commands =
		[
        Run StdinReader
        , Run Network "enp3s0" ["-t", "<fc=#26f02c>上:<tx></fc> <fc=#eb2828>下:<rx></fc>"
                  , "--minwidth", "8"
                  , "--maxwidth", "8"
                , "-S", "True"] 10
      , Run Memory [ "-t", "<fn=1></fn> <usedratio>%" ] 10
      -- always 0% anyways , Run Swap [] 10
      , Run MultiCpu ["-t", "<fn=1></fn> <total>%" ] 10     
      , Run Date "<fc=#ffb044><fn=1></fn> %a %b %d %T %Z</fc>" "date" 10
		]
}
